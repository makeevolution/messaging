{
  "Messaging": {
    "HostName": "rabbitmq",
    "ExchangeName": "orders"
  },
  "ConnectionStrings": {
    "OrdersContext": "Host=postgres;Username=postgres;Password=pass;Database=mydb;"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://ankiai.seq:5341"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "Application": "Producer.Api"
    }
  }

}
